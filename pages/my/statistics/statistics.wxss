.phone_one {
  /* //用flex布局更方便。 */
  display: flex;
  position: relative;
  justify-content: space-between;
  background-color: #fff;
  width: 100%;
  height: 80rpx;
  margin: 0 auto;
  border-bottom: 2rpx solid rgb(255, 255, 255);
}

.line0 {
  width: 25%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.phone_personal {
  text-overflow: ellipsis;
  width: 4rem;
  color: #333;
  white-space: nowrap;
  height: 80rpx;
  line-height: 100rpx;
  text-align: center;
  background-color: #fff;
  font-size: 28rpx;
}

.line {
  width: 1rpx;
  height: 80rpx;
  background-color: #d7d7d7;
}

.person_box {
  position: relative;
  border-bottom: 1rpx solid #d7d7d7;
}

.phone_select {
  width: 100%;
  margin-top: 0;
  z-index: 100;
  background-color: #f8f8f8;
  /* //小程序中z-index和absolute需要同时存在，元素才能脱离文档。 */
  position: absolute;
}

.select_one {
  text-align: center;
  /* //脱离文档后元素width不能再用百分比。 */
  width: 676rpx;
  height: 100rpx;
  line-height: 100rpx;
  margin: 0 5%;
  border-bottom: 2rpx solid #d7d7d7;
}

.personal_image {
  z-index: 100;
  width: 28rpx;
  height: 28rpx;
  margin-left: 20rpx;
  margin-top: 10rpx;
  transition: All 0.4s ease;
  -webkit-transition: All 0.4s ease;
}

.rotateRight {
  /* //180°旋转图片。 */
  transform: rotate(180deg);
}
